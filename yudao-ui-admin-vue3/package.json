    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "pinia-plugin-persist": "^1.0.0",
    "qrcode": "^1.5.1",
    "qs": "^6.11.0",
    "vue": "3.2.37",
    "vue-cropper": "^1.0.3",
    "vue-i18n": "9.1.10",
    "vue-router": "^4.1.2",
    "vue-types": "^4.1.1",
    "web-storage-cache": "^1.1.1"
  },
