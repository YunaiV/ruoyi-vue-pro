      },
      "./store": {
        "types": "./dist/store.d.ts",
        "default": "./dist/store.mjs"
      },
      "./global-state": {
        "types": "./dist/global-state.d.ts",
        "default": "./dist/global-state.mjs"
      }
    }
  },
  "dependencies": {
    "@ctrl/tinycolor": "catalog:",
    "@tanstack/vue-store": "catalog:",
    "@vue/shared": "catalog:",
    "clsx": "catalog:",
    "crypto-js": "catalog:",
