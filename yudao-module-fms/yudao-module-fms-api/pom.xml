    <modelVersion>4.0.0</modelVersion>
    <artifactId>yudao-module-fms-api</artifactId>
    <packaging>jar</packaging>

    <name>${project.artifactId}</name>
    <description>
        fms 模块 API，暴露给其它模块调用,销售
    </description>

    <dependencies>
